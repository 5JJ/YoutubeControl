@section scripts {
    @Scripts.Render("~/bundles/app")
}
@Styles.Render("~/Content/Site.css")
<h2>Video</h2>
<div id="vPlay">
</div>




<div style="float:left;margin-right:100px;">
    <div class="panel-heading">
        <h2 class="panel-title">Lists</h2>
        @*<button id="AddList">+</button>*@
    </div>
    <div class="form-group" data-bind="with: newListBox">
        <label for="CreateListBox">Name :</label>
        <input type="text" id="CreateListBox" />
        <input type="button" value="Add" onclick="inputListBox();" />
    </div>
    <div>
        <ul class="list-unstyled" id="listBox"></ul>
    </div>
</div>
<div style="float:left;">
    <div class="panel-heading">
        <h2 class="panel-title">Videos</h2>
    </div>
    <div>
        <label for="youtubeURL">URL :</label>
        <input type="text" id="youtubeURL" />
        <input type="button" value="INPUT" onclick="addVideo();" />
    </div>
    <div class="panel-body">
        <ul class="list-unstyled" id="videos"></ul>
    </div>
</div>
